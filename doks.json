{
  "language": "escapedCoffee",
  "glob": "+(src|ext)/**/*.coffee",
  "arrayTags": [
    "example",
    "reward",
    "prerequisite",
    "statPerLevel",
    "effect",
    "syntax",
    "requirement"
  ],
  "json": "config/*.json",
  "outputOnly": true,
  "templateOptions": {
    "page": {
      "pageName": "IdleLands"
    },
    "content": {
      "sourceLink": "https://github.com/IdleLands/IdleLands/tree/master/%filePath#L%lineNumber-L%endLineNumber"
    }
  }
}